{
  "name": "javascript-refresh",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "tsc": "tsc",
    "build": "npm-run-all --parallel tsc copy",
    "copy": "ts-node copyStaticAssets.ts",
    "ts-dev": "nodemon src/local-server.ts",
    "dev": "set NODE_ENV=development&set NODE_SCOPE=local&npm-run-all --parallel ts-dev watch-sass",
    "watch-sass": "sass --watch src\\public\\stylesheets:dist\\public\\stylesheets"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "create-serve": "^1.0.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "esbuild": "^0.14.39",
    "express": "^4.17.3",
    "express-ejs-layouts": "^2.5.1",
    "highlight.js": "^11.5.1",
    "marked": "^4.0.14",
    "mongoose": "^6.3.4",
    "serverless-http": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.27",
    "connect-livereload": "^0.6.1",
    "livereload": "^0.9.3",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.50.1",
    "shelljs": "^0.8.5",
    "typescript": "^4.6.3"
  }
}
