{
  "name": "javascript-refresh",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node out/server/app.js",
    "tsc": "tsc",
    "build": "npm-run-all --parallel tsc copy",
    "copy": "ts-node copyStaticAssets.ts",
    "build:client": "node config/client.build.js",
    "build:server": "node config/server.build.js",
    "ts-dev": "set NODE_ENV=development&set NODE_SCOPE=local&nodemon src/local-server.ts",
    "ts-dev-fn": "set NODE_ENV=development&set NODE_SCOPE=local&nodemon functions/server.ts",
    "dev": "set NODE_ENV=development&set NODE_SCOPE=local&npm-run-all --parallel ts-dev watch-sass",
    "dev2": "set NODE_ENV=development&set NODE_SCOPE=local&npm-run-all --parallel build:server build:client",
    "dev3": "set NODE_ENV=development&set NODE_SCOPE=local&npm-run-all --parallel browser:sync ts-dev",
    "watch-sass": "sass --watch src\\public\\stylesheets:dist\\public\\stylesheets",
    "browser:sync": "browser-sync start --config bs-config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "create-serve": "^1.0.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "esbuild": "^0.14.39",
    "esbuild-sass-plugin": "^2.2.6",
    "express": "^4.17.3",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "highlight.js": "^11.5.1",
    "marked": "^4.0.14",
    "mongoose": "^6.3.4",
    "preact": "^10.7.2",
    "preact-router": "^4.0.1",
    "react": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "serverless-http": "^3.0.1",
    "ts-node": "^10.8.0",
    "wouter": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.27",
    "browser-sync": "^2.27.10",
    "connect-livereload": "^0.6.1",
    "livereload": "^0.9.3",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.50.1",
    "shelljs": "^0.8.5",
    "typescript": "^4.6.3"
  }
}
